@inproceedings{palacios_serrano_high_2021,
	title = {High voltage design and evaluation of Wien filters for the {CEBAF} 200 {keV} injector upgrade},
	url = {http://dx.doi.org/10.18429/JACOW-IPAC2021-MOPAB324},
	doi = {10.18429/JACOW-IPAC2021-MOPAB324},
	abstract = {High-energy nuclear physics experiments at the Jefferson Lab Continuous Electron Beam Accelerator Facility ({CEBAF}) require highly
spin-polarization electron beams, produced from strained super-lattice {GaAs} photocathodes, activated to negative electron affinity in a photogun operating at 130 {kV} dc. A pair of Wien filter spin rotators in the injector defines the orientation of the electron beam polarization at the end station target. An upgrade of the {CEBAF} injector to better support the upcoming {MOLLER} experiment requires increasing the electron beam energy to 200 {keV}, to reduce unwanted helicity correlated intensity and position systematics and provide precise control of the polarization orientation. Our contribution describes design, fabrication and testing of the high voltage system to upgrade the Wien spin rotator to be compatible with the 200 {keV} beam. This required Solidworks modeling, {CST} and Opera electro- and magnetostatic simulations, upgrading {HV} vacuum feedthroughs, and assembly techniques for improving electrode alignment. The electric and magnetic fields required by the Wien condition and the successful {HV} characterization under vacuum conditions are also presented.},
	publisher = {{JACoW} Publishing, Geneva, Switzerland},
	author = {Palacios Serrano, Gabriel and Adderley, Philip and Baumgart, Helmut and Benesch, Jay and Bullard, Don and Grames, Joseph and Hernandez-Garcia, Carlos and Hofler, Alicia and Machie, Danny and Poelker, Matt and Stutzman, Marcy and Suleiman, Riad},
	date = {2021-09-09},
}

